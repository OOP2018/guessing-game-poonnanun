<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="applicationUI.MainUIController">
   <center>
      <FlowPane alignment="CENTER" prefHeight="109.0" prefWidth="381.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="guide" alignment="CENTER" prefHeight="39.0" prefWidth="279.0" text="What is your maximum number?">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <TextField fx:id="number" prefHeight="42.0" prefWidth="167.0">
               <FlowPane.margin>
                  <Insets bottom="10.0" />
               </FlowPane.margin>
            </TextField>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </FlowPane>
   </center>
   <bottom>
      <FlowPane alignment="CENTER" prefHeight="44.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="play" alignment="CENTER" mnemonicParsing="false" onAction="#playClick" text="Play" textAlignment="CENTER" />
            <Button fx:id="quit" mnemonicParsing="false" onAction="#quitGame" text="Quit">
               <FlowPane.margin>
                  <Insets left="20.0" />
               </FlowPane.margin>
            </Button>
         </children>
      </FlowPane>
   </bottom>
   <top>
      <FlowPane alignment="CENTER" prefHeight="46.0" prefWidth="381.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Ou's Guessing Game">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
         </children>
      </FlowPane>
   </top>
</BorderPane>
